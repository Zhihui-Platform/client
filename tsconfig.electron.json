{
  "include": ["src/app/*", "src/app/**/*"],
  "exclude": ["src/app/**/*spec.ts", "src/app/**/*e2e-spec.ts"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015"],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  }
}
